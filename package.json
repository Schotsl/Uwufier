{
  "name": "@schotsl/uwuifier",
  "version": "2.0.4",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "unpkg": "dist/index.umd.js",
  "author": "Sjors van Holst",
  "license": "MIT",
  "homepage": "https://github.com/Schotsl/uwuifier#readme",
  "repository": "git://github.com/Schotsl/Uwuifier.git",
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "bugs": {
    "url": "https://github.com/Schotsl/uwuifier/issues"
  },
  "keywords": [
    "Uwuifier",
    "Sentence",
    "Anime",
    "Faces",
    "Weeb",
    "UwU",
    "OwO"
  ],
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "concurrently": "^5.3.0",
    "eslint": "^7.19.0",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "rollup": "^2.38.3",
    "ts-jest": "^26.4.0",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "lint": "eslint",
    "test": "npx jest",
    "serve": "nodemon",
    "build": "concurrently \"npm:lint\" \"npm:format\" \"npm:test\" \"tsc\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "concurrently \"npm:build\" \"rollup -c\""
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run build",
    "ext": "tsc"
  }
}
